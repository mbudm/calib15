    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mbudm/calib15.git"
  },
  "keywords": [
    "xlabs",
    "calibration",
    "gaze-tracking",
    "chrome",
    "browser",
    "amd"
  ],
  "author": {
    "name": "Steve Robertsn",
    "email": "steve@xlabs.com.au",
    "url": "http://xlabsgaze.com/"
  },
  "license": "Unlicense",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/mbudm/calib15/issues"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "plugin": {
      "exports": "plugin",
      "depends": [
        "jquery:$"
      ]
    }
  },
  "dependencies": {
    "xLabsApi": "mbudm/xLabsApi.git#0.1.0",
    "event-emitter": "~0.3.1",
    "jquery": "~2.1.3"
  },
  "devDependencies": {
    "brfs": "^1.4.0",
    "browserify": "^9.0.3",
    "browserify-shim": "^3.8.3",
    "faucet": "0.0.1",
    "gulp": "^3.8.11",
    "gulp-jshint": "^1.9.4",
    "gulp-shell": "^0.4.0",
    "gulp-sourcemaps": "^1.5.1",
    "gulp-uglify": "^1.1.0",
    "gulp-util": "^3.0.4",
    "shelljs": "^0.4.0",
    "tape": "^3.5.0",
    "testling": "^1.7.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "vinyl-transform": "^1.0.0",
    "watchify": "^2.6.0"
  },
  "testling": {
    "files": "test/all.js"
  },
  "scripts": {
    "test": "tape test/node.js"
  }
